{
    "name": "rental-management/rental-system",
    "description": "Rental Management System built with CakePHP",
    "type": "project",
    "keywords": ["cakephp", "rental", "management"],
    "homepage": "https://cakephp.org",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "^4.5",
        "cakephp/migrations": "^3.0",
        "cakephp/plugin-installer": "^1.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "cakephp/bake": "^2.6",
        "cakephp/debug_kit": "^4.6",
        "cakephp/authentication": "^2.2",
        "cakephp/authorization": "^2.4",
        "cakephp/orm": "^4.0",
        "tecnickcom/tcpdf": "^6.6",
        "google/apiclient": "^2.15",
        "firebase/php-jwt": "^6.9",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.6",
        "phpunit/phpunit": "^9.5",
        "cakephp/bake": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@test",
            "@cs-check"
        ],
        "cs-check": "phpcs --colors -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "cs-fix": "phpcbf --colors --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
        "test": "phpunit --colors=always"
    },
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
